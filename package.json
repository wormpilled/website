{
	"name": "wormpilled",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"predev": "node scripts/build-posts.js",
		"dev": "concurrently \"npm:watch:posts\" \"vite dev\"",
		"watch:posts": "nodemon --watch /home/user/Obsidian/wormpilled/POSTS --watch /home/user/Obsidian/wormpilled/UPLOADS -e md,png,jpg,jpeg,gif --exec \"node scripts/build-posts.js\"",
		"build": "node scripts/build-posts.js && vite build",
		"preview": "vite preview"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "3.0.1",
		"@sveltejs/kit": "2.5.7",
		"@sveltejs/vite-plugin-svelte": "3.1.0",
		"concurrently": "8.2.2",
		"gh-pages": "6.1.1",
		"gray-matter": "4.0.3",
		"marked": "13.0.1",
		"nodemon": "3.1.2",
		"svelte": "4.2.18",
		"vite": "5.2.12"
	}
}